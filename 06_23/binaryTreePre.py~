class Solution:
    def preOrder(self, root):
        if not root:
            return []
        q = []
        ans = []
        
        q.append(root)
        
        while q:
            node = q.pop()
            ans.append(node.val)
            if node.right:
                ans.append(node.right)
            if node.left:
                ans.append(node.left)
        
        return ans

