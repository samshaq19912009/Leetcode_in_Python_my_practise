class Solution:
    #Given n and k, return all possible combinations of k numbers
    #within n
    def combinations(self, n, k):
        if k == 1:
            return [i+1 for i in range(n)]

        if n > k:
            return [ n + self.combinations(n-1, k-1)] + [ n + self.combinations(n-1,k)]
        else:
            return [ n + self.combinations(n-1, k-1)]

