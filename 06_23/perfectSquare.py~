class Solution:
    def numSquares(self, n):
        """
        :type n: int
        :rtype: int
        """
        dp = [ 100000] * (n+1)
        
        dp[1] = 1
        for i in range(1, n+1):
            j = 1
            while j*j <= i:
                if j*j == i:
                    dp[i] = 1
                    break
                dp[i] = min(dp[i], dp[i-j*j]+1)
                j += 1
                
        return dp[n]
        
    def numSqures(self,n)
