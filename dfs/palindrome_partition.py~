class Solution:
    def partition(self, s):
        ans = []
        n = len(s)
        self.dfs(0, n, ans,s)
        return ans

    def dfs(self, start, end, ans,s):
        if start >= end:
            return 
        for i in range(start, end):
            tmp = s[start:i+1]
            if self.isP(tmp):
                ans.append(tmp)

        
    def isP(self, str):
        return str == str[::-1]
